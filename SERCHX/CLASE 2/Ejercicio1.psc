Proceso Calculadora
	Escribir "El primer numero:"
	Leer valor1
	
	Escribir "El segundo numero:"
	Leer valor2
	
	//suma
	calculo = valor1 + valor2
	Escribir "El valar de la suma es:", calculo
	
	//resta
	calculo = valor1 - valor2
	Escribir "El valar de la resta es:", calculo
	
	//division
	calculo = valor1 / valor2
	Escribir "El valar de la division es:", calculo
	
	//multiplicacion
	calculo = valor1 * valor2
	Escribir "El valar de la multiplicacion es:", calculo
	
FinProceso
